local function check()
    for i , v in pairs(plr.PlayerStats:GetChildren()) do
        if string.match(v.Name , "Level") then
            local le = v.Value 
        if plr.PlayerStats.Prestige.Value <= 2 then
            if le >= 1 and le < 10 then 
                dialogue = {["Option"] = "Option1",["Dialogue"] = "Dialogue5",["NPC"] = "Officer Sam"}
                dialogue2 = {["Option"] = "Option1",["Dialogue"] = "Dialogue5",["NPC"] = "Officer Sam"}
                reward = "Take down 5 thugs"
                mon = "Thug"
            elseif le >= 10 and le < 15 then
                dialogue ={["Option"] = "Option1",["Dialogue"] = "Dialogue2",["NPC"] = "Deputy Bertrude"}
                dialogue2 ={["Option"] = "Option1",["Dialogue"] = "Dialogue5",["NPC"] = "Deputy Bertrude"}
                reward = "Take down 5 corrupt police"
                mon =  "Corrupt Police"
            elseif le >= 15 and le < 20 then
                dialogue = {["Dialogue"] = 'Dialogue6',["NPC"] = "Lion Abbacho's Partner",["Option"] = 'Option1'}
                dialogue2 = {["Dialogue"] = 'Dialogue7',["NPC"] = "Lion Abbacho's Partner",["Option"] = 'Option1'}
                reward =  "Take down 5 alpha thugs"
                mon =  "Alpha Thug"
            elseif le >= 20 and le < 30 then
                dialogue = {["Dialogue"] = 'Dialogue4',["NPC"] = 'Dracula',["Option"] = 'Option1'}
                dialogue2 = {["Dialogue"] = 'Dialogue4',["NPC"] = 'Dracula',["Option"] = 'Option1'}
                reward = "Take down 3 zombie henchmans"
                mon =  "Zombie Henchman"
            elseif le >= 30 and le < 35 then
                dialogue =  {["Dialogue"] = 'Dialogue3',["NPC"] = 'Dopey',["Option"] = 'Option1'}
                dialogue2 =  {["Dialogue"] = 'Dialogue5',["NPC"] = 'Dopey',["Option"] = 'Option1'}
                reward = "Kill Deo"
                mon =   "Deo" 
            elseif le >= 35 and le < math.huge then
                dialogue =  {["Dialogue"] = 'Dialogue4',["NPC"] = 'Deo',["Option"] = 'Option1'}
                reward = "Defeat Joe"
                mon =   "Joe" 
--[[
