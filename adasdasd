local function autofarm()
    if not plr.PlayerGui.HUD.Main.Frames.Quest.Quests:FindFirstChild(reward) then
         game.Players.LocalPlayer.Character.RemoteEvent:FireServer("EndDialogue",dialogue)      
         game.Players.LocalPlayer.Character.RemoteEvent:FireServer("EndDialogue",dialogue2)                  
     elseif plr.PlayerGui.HUD.Main.Frames.Quest.Quests:FindFirstChild(reward)  then
        for i , v in pairs(game:GetService("Workspace").Living:GetChildren()) do 
             if v.Name == mon then
                    if  tonumber(v.Health.Value) > 0 or v.Humanoid.Health > 1  then
                         repeat  wait()
                                if set.mode == "Next to" then
                                    Tp(v.HumanoidRootPart.CFrame * CFrame.new(0,1,-set.distants))
                        
                                end
                                if set.mode == "Behind" then
                                    Tp(v.HumanoidRootPart.CFrame * CFrame.new(0,1,set.distants))
                                end
                                plr.Character.HumanoidRootPart.CFrame = CFrame.new(plr.Character.HumanoidRootPart.Position, v.HumanoidRootPart.Position)
                             if (v.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).magnitude < 30 then
                                 Click()
                                 if set.e then
                                     plr.Character.RemoteEvent:FireServer("InputBegan",{["Input"] = Enum.KeyCode.E})
                                 end
                                 if set.r then
                                     plr.Character.RemoteEvent:FireServer("InputBegan",{["Input"] = Enum.KeyCode.R})
                                 end
                                 if set.t then
                                     plr.Character.RemoteEvent:FireServer("InputBegan",{["Input"] = Enum.KeyCode.T})
                                 end
                                 if set.z then
                                    plr.Character.RemoteEvent:FireServer("InputBegan",{["Input"] = Enum.KeyCode.Z})
                                end
                                if set.y then
                                    plr.Character.RemoteEvent:FireServer("InputBegan",{["Input"] = Enum.KeyCode.Y})
                                end
                                if set.h then
                                    plr.Character.RemoteEvent:FireServer("InputBegan",{["Input"] = Enum.KeyCode.H})
                                end
                                if set.x then
                                    plr.Character.RemoteEvent:FireServer("InputBegan",{["Input"] = Enum.KeyCode.X})
                                end
                                if set.u then
                                    plr.Character.RemoteEvent:FireServer("InputBegan",{["Input"] = Enum.KeyCode.U})
                                end
                                if set.j then
                                    plr.Character.RemoteEvent:FireServer("InputBegan",{["Input"] = Enum.KeyCode.J})
                                end
                                if set.g then
                                    plr.Character.RemoteEvent:FireServer("InputBegan",{["Input"] = Enum.KeyCode.G})
                                end
                             end   
                          --   if (v.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).magnitude < 30 then
                                for i2 , v2 in pairs(game:GetService("Workspace")["Mob_Spawns"]:GetChildren()) do
                                    if v2.Name == mon then
                                        if v2:IsA("Part") then
                                            Tp(v.CFrame * CFrame.new(0,1,-set.distants))
                                        end
                                    end
                                end
                         --   end

                         until tonumber(v.Health.Value) == 0 or v.Humanoid.Health == 0  or  not set.farm or  not plr.PlayerGui.HUD.Main.Frames.Quest.Quests:FindFirstChild(reward)             
                        end
                    end
                end
            end
   if plr.PlayerStats.Prestige.Value <= 3 then
      if  set.rebirth then 
        for i = 0 , 5 , 1 do wait()
            if i == 5 then
                return 0
            end
            plr.Character.RemoteEvent:FireServer("EndDialogue",{["Dialogue"] = 'Dialogue'..tostring(i) ,["NPC"] = 'Prestige',["Option"] = 'Option1'})
            end
        end
    end
end


local tpitemss = function ()
    if not game:GetService("Workspace")["Item_Spawns"].Items:FindFirstChild("Model")  then
        -- ex mata 
                setreadonly(Vector3, false)
                local vt3 = Vector3.new()
                local mt = getrawmetatable(vt3)
                setreadonly(mt, false)
                local old4 = mt.__index
                mt.__index = newcclosure(function(a,b,c)
                local method = getcallingscript()
                if not checkcaller() 
                        and method == game.ReplicatedFirst.ItemSpawn
                        and tostring(b) == "magnitude" then
                            return 0
                        end
                        return old4(a, b, c)
                end) 
        --end mata 
            elseif   game:GetService("Workspace")["Item_Spawns"].Items.Model:FindFirstChild("MeshPart") then
                for i , v in pairs(game:GetService("Workspace")["Item_Spawns"].Items:GetChildren()) do 
                    if v:IsA("Model") then
                        if v:FindFirstChild("MeshPart") and v:FindFirstChild("ClickDetector") or v:IsA("Part") then
                            repeat wait()
                               game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.MeshPart.CFrame * CFrame.new(0,3,0)
                          
                               if (v.MeshPart.Position -  game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude < 5 then
                                wait(2)
                                fireclickdetector(v.ClickDetector)
                            end
--[[
                                if (v.Part.Position -  game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude < 5 then
                                wait(1)
                                fireclickdetector(v.ClickDetector)
                            end
]]
                            -- if game:GetService("Workspace")["Item_Spawns"].Items.Model:FindFirstChild("Part") or not  game:GetService("Workspace")["Item_Spawns"].Items.Model:FindFirstChild("MeshPart") then 
                            --     game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Part.CFrame* CFrame.new(0,3,0)
                            --     if (v.Part.Position -  game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude < 5 then
                            --         wait(2)
                            --         fireclickdetector(v.ClickDetector)
                            --     end
                            -- end
                            until  not    game:GetService("Workspace")["Item_Spawns"].Items.Model:FindFirstChild("MeshPart") or not set.itemfarm
                        end
                    end
                end
            end
end


local itemsserv = function ()
    if not   game:GetService("Workspace")["Item_Spawns"].Items.Model:FindFirstChild("MeshPart") then
        Teleport()
    end
end


