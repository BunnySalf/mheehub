                local url =  setting.urlhook
                local data = {
                ["content"] = "@everyone",
                ["embeds"] = {
                    {
                        ["title"] = "**Notify**",
                        ["description"] = "Items  : " .."**".. a.Name.."**",
                        ["type"] = "rich",
                        ["color"] = tonumber(0x7269da),
                         
                                ["image"] = {
                            ["url"] = "https://cdn.discordapp.com/attachments/688002903675371592/850358891878678528/GomuGomunoMi.png"
                        }
                         
                    
                    }
                }
                }
                local newdata = game:GetService("HttpService"):JSONEncode(data)
    
                local headers = {
                ["content-type"] = "application/json"
                }
                request = http_request or request or HttpPost or syn.request
                local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
                request(abcdef)
            end
        end)
    end
end)

